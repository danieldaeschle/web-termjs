{
  "name": "web-term",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [
    "util",
    "functional",
    "browser",
    "terminal",
    "console",
    "shell"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf _bundles lib lib-esm",
    "build:sass": "node node_modules/node-sass/bin/node-sass --output-style compressed src/terminal.scss > dist/terminal.css",
    "build:tsc": "tsc && tsc -m es6 --outDir lib-esm && webpack",
    "build": "npm run clean && npm run build:tsc && npm run build:sass"
  },
  "directories": {
    "src": "src"
  },
  "devDependencies": {
    "@types/node": "^8.0.51",
    "awesome-typescript-loader": "^3.3.0",
    "node-sass": "^4.6.1",
    "shx": "^0.2.2",
    "tsc": "^1.20150623.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  }
}
